<% provide(:title, 'Society - ' + @society.name) %>
<div class="container">
<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @society.name %>
</p>

<p>
  <strong>Description:</strong>
  <%= @society.description %>
</p>

<p>
  <strong>Events:</strong>
  <% @society.events.each do|event| %>
    <li><%=event.name%></li>
  <%end%>
</p>

<!--
<p>
  <strong>Website:</strong>
  <a href= <%= "\"" + @society.customWebsite + "\""%> > <%=@society.customWebsite%></a>
</p>
-->

<p>
  <strong>Membershipfee:</strong>
  <%= @society.membershipFee %>
</p>

<p>
  <strong>Registration Number:</strong>
  <%= @society.registrationNumber %>
</p>

<p>
  <strong>Verified:</strong>
  <%= @society.verified %>
</p>

<% if user_signed_in? %>
	<%= link_to 'Edit', edit_society_path(@society) %> |
<% end %>
<%= link_to 'Back', :back %>
</div>